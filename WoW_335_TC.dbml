  "uptime" int(10) [not null, default: "0"]
  "maxplayers" smallint(5) [not null, default: "0"]
  "revision" varchar(255) [not null, default: "Trinitycore"]

Indexes {
  (realmid, starttime) [pk]
}
  Note: 'Uptime system'
}

Enum "Characters"."updates_state_enum" {
  "RELEASED"
  "ARCHIVED"
}

Enum "Characters"."updates_include_state_enum" {
  "RELEASED"
