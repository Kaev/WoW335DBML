Table "Auth"."updates" { 
  "name" varchar(200) [pk, not null, note: 'filename with extension of the update.']
  "hash" char(40) [default: "", note: 'sha1 hash of the sql file.']
  "state" "Auth"."updates_state_enum" [not null, default: "RELEASED", note: 'defines if an update is released or archived.']
  "timestamp" timestamp [not null, default: `CURRENT_TIMESTAMP`, note: 'timestamp when the query was applied.']
  "speed" int(10) [not null, default: "0", note: 'time the query takes to apply in ms.']
  Note: 'List of all applied updates in this database.'
}

Table "Auth"."updates_include" { 
  "path" varchar(200) [pk, not null, note: 'directory to include. $ means relative to the source directory.']
  "state" "Auth"."updates_include_state_enum" [not null, default: "RELEASED", note: 'defines if the directory contains released or archived updates.']
  Note: 'List of directories where we want to include sql updates.'
}

Table "Auth"."uptime" { 
  "realmid" int(10) [not null]
